OPENMP='-fopenmp'
SOMP='-fopenmp'
F77='-fixed -warn nogeneral'
F90='-warn nogeneral'
F95='-warn nogeneral'
NOFMAIN='-nofor-main'
CCONDEF=
USUAL=
SPECIAL=
F77SUFFIX=f90
F95SUFFIX=f90
CUDA=
CUDACOMP=$FORTRAN
CUDAARCH=
CUDACODE=
CUDABASIC=
CUDALIBS=
